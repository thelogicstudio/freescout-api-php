{
    "name": "thelogicstudio/freescout-api",
    "type": "library",
    "description": "Free Scout API Client",
    "homepage": "https://github.com/thelogicstudio/freescout-api-php",
    "keywords": ["freescout", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "The Logic Studio",
            "homepage": "https://logicstudio.nz"
        },
        {
            "name": "Help Scout Platform Team",
            "email": "developer@helpscout.net",
            "homepage": "https://www.helpscout.net"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "webmozart/assert": "^1.2",
        "guzzlehttp/guzzle": "^6.3|^7.0.1",
        "rize/uri-template": "^0.3.5|^0.4.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.4",
        "mockery/mockery": "^1.1",
        "ramsey/uuid": "^4.1"
    },
    "autoload": {
        "psr-4": { "FreeScout\\Api\\": "src/" }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse src tests --level=5",
        "fix": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "sniff": "vendor/bin/php-cs-fixer fix --allow-risky=yes --dry-run --verbose"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
